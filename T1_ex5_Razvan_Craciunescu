A=1.5;
w=2*pi*0.25;
t1=0:0.2:10;                   
s1=abs(A*sin(w*t1));  %prin abs transformam alternantele negative in pozitive
subplot(3,1,1)
plot(t1,s1),grid
xlabel('Timp[secunde]')
ylabel('Amplitudine')

t2=0:0.02:10;
s2=abs(A*sin(w*t2));
subplot(3,1,2)
plot(t2,s2),grid
xlabel('Timp[secunde]')
ylabel('Amplitudine')

t3=0:0.002:10;
s3=abs(A*sin(w*t3));
subplot(3,1,3)
plot(t3,s3),grid
xlabel('Timp[secunde]')
ylabel('Amplitudine')
